const axios = require('axios');

const baseUrl = 'http://192.168.2.142:80';

async function verificarUserGroups() {
  try {
    const login = await axios.post(
      `${baseUrl}/login.fcgi`,
      { login: 'admin', password: 'admin' },
      { headers: { 'Content-Type': 'application/json' }, timeout: 10000 }
    );

    const session = login.data.session;

    const userGroups = await axios.post(
      `${baseUrl}/load_objects.fcgi?session=${session}`,
      { object: 'user_groups' },
      { headers: { 'Content-Type': 'application/json' }, timeout: 10000 }
    );

    console.log('üìã Rela√ß√µes user_groups na Control ID:\n');
    if (userGroups.data.user_groups && userGroups.data.user_groups.length > 0) {
      userGroups.data.user_groups.forEach(ug => {
        console.log(`  user_id: ${ug.user_id}, group_id: ${ug.group_id}`);
      });
    } else {
      console.log('  Nenhuma rela√ß√£o user_groups encontrada');
    }

  } catch (error) {
    console.error('‚ùå Erro:', error.message);
  }
}

verificarUserGroups();
